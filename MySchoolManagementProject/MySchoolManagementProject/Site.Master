                                <ContentTemplate>
                                    <ul class="nav navbar-nav navbar-right">
                                         <li>
                                             <a class="logout" runat="server" href="./Pages/Dashboard" title="Manage your account">Admin Deshboard</a></li>
                                        <li>
                                            <asp:LoginStatus runat="server" LogoutAction="Redirect" LogoutText="Log off" LogoutPageUrl="~/" OnLoggingOut="Unnamed_LoggingOut" ForeColor="Black" />
                                        </li>
                                    </ul>
                                </ContentTemplate>
                            </asp:RoleGroup>
                        </RoleGroups>  


                    </asp:LoginView>
